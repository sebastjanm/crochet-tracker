{
  "expo": {
    "name": "Artful Space Crochet Tracker",
    "slug": "crochet-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "artfulspace",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#E891A0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.sebastjanm-crochet-tracker",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "UIBackgroundModes": [
          "audio"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.sebastjanm.crochettracker",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://www.artful.space/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you add images to your projects and inventory items.",
          "cameraPermission": "The app accesses your camera to let you take photos of your crochet projects and supplies."
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-asset",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "crochet-tracker",
          "organization": "sebom"
        }
      ],
      "@sentry/react-native",
      "expo-video",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/3e109c0c-7948-4df6-8908-5e886b950879"
    },
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "extra": {
      "router": {
        "origin": "https://www.artful.space/"
      },
      "eas": {
        "projectId": "3e109c0c-7948-4df6-8908-5e886b950879"
      }
    },
    "owner": "sebastjanm"
  }
}
